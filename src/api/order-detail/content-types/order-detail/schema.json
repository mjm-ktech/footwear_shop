{
  "kind": "collectionType",
  "collectionName": "order_details",
  "info": {
    "singularName": "order-detail",
    "pluralName": "order-details",
    "displayName": "Order detail"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "quantity": {
      "type": "integer",
      "default": 0
    },
    "unit_price": {
      "type": "biginteger",
      "default": "0"
    },
    "note": {
      "type": "text"
    },
    "product": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product.product"
    },
    "order": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::order.order",
      "inversedBy": "order_details"
    }
  }
}
